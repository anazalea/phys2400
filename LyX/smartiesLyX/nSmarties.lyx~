\begin_inset Formula $n_{\mathrm{Smarties}}=\frac{X-Y-Z}{W}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $n_{\mathrm{Smarties}}=\#$
\end_inset

.
\end_layout

